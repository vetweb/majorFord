    <script type="text/javascript">
        ymaps.ready(init);

        function init() {
            var myMap = new ymaps.Map("map", {
                    center: ['55.789124', '37.250648'],
                    zoom: 16
                }, {
                    searchControlProvider: 'yandex#search'
                }),

                // Создаем геообъект с типом геометрии "Точка".
                myGeoObject = new ymaps.GeoObject({
                    // Описание геометрии.
                    geometry: {
                        type: "Point",
                        coordinates: ['55.789124', '37.250648']
                    },
                    // Свойства.
                    properties: {
                        // Контент метки.
                        iconContent: 'MAJOR TRUCK CENTER'
                    }
                }, {
                    // Опции.
                    // Иконка метки будет растягиваться под размер ее содержимого.
                    preset: 'islands#blackStretchyIcon',
                    draggable: true
                });

            myMap.geoObjects
                .add(myGeoObject)
                .add(new ymaps.Placemark(['55.789124', '37.250648'], {
                    balloonContent: '<strong>MAJOR TRUCK CENTER</strong>'
                }, {
                    preset: 'islands#blueCircleDotIconWithCaption',
                    iconCaptionMaxWidth: '50'
                }));
        }

